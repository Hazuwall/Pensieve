<?xml version="1.0" encoding="utf-8"?>
<Package xmlns="http://schemas.microsoft.com/appx/2010/manifest" xmlns:m2="http://schemas.microsoft.com/appx/2013/manifest">
  <Identity Name="Pensieve" Publisher="CN=Hazuwall" Version="1.0.0.4" />
  <Properties>
    <DisplayName>Pensieve</DisplayName>
    <PublisherDisplayName>Hazuwall</PublisherDisplayName>
    <Logo>Assets\StoreLogo.png</Logo>
  </Properties>
  <Prerequisites>
    <OSMinVersion>6.3.0</OSMinVersion>
    <OSMaxVersionTested>6.3.0</OSMaxVersionTested>
  </Prerequisites>
  <Resources>
    <Resource Language="x-generate" />
  </Resources>
  <Applications>
    <Application Id="App" Executable="$targetnametoken$.exe" EntryPoint="Pensieve.App">
      <m2:VisualElements DisplayName="Pensieve" Square150x150Logo="Assets\Logo.png" Square30x30Logo="Assets\SmallLogo.png" Description="Pensieve" ForegroundText="light" BackgroundColor="#0d3a47">
        <m2:DefaultTile ShortName="Pensieve">
          <m2:ShowNameOnTiles>
            <m2:ShowOn Tile="square150x150Logo" />
          </m2:ShowNameOnTiles>
        </m2:DefaultTile>
        <m2:SplashScreen Image="Assets\SplashScreen.png" BackgroundColor="#0d3a47" />
      </m2:VisualElements>
    </Application>
  </Applications>
</Package>